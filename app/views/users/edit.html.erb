<h1>ユーザ編集</h1>

<%= form_for @user do |form| %>
  <div  class="form-group">
    <label for="account">アカウント</label>
    <%= form.text_field :account, :class => "form-control" %>
  </div>
  <div  class="form-group">
    <label>メールアドレス</label>
    <%= form.text_field :mail_address, :class => "form-control"%>
  </div>
  <%= form.submit '登録', :class => "btn btn-danger" %>
<% end %>
